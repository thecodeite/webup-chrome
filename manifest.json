{
  "manifest_version": 2,

  "name": "Web status icon",
  "description": "This extension shows the current status of the internet",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "pause.png"
  },
  "permissions": [
    "activeTab",
    "https://status.cloud.google.com/"
  ]
}